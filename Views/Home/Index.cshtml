@model IEnumerable<BigSchool.Models.Course>
@{
    ViewBag.Title = "Home Page";
}

<div class="row">
    <ul>
        @foreach (var course in Model)
        {
            <li>
                @course.DateTime.ToString("dd/MM/yyyy HH:mm") - @course.Category.Name
            </li>
        }
    </ul>
</div>